#include <stdio.h>
int main(){
   int n, i = 0;
   printf("Введите целое число сомножителей: ");
   scanf("%d", &n);
   float c, sum;
   if(n > 0){
      sum = 1;
      c = 1;
      for (int i = 0; i < n; i++){
         c += 0.1;
         sum = sum * c;
      }
   printf("%g", sum);
   return 0;
   }
   else{
      printf("Error!");
      return 0;
   }
}
