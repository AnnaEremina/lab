#include <stdio.h>
int main(){
   int a, b, i, j;
   printf("Введите два целый положительных числа А и В: ");
   scanf("%d %d", &a, &b);
   if(a>=b){
      printf("Неверные границы!");
      return 0;
   }
   else{
      for (int c=a;c<=b;c++){
         for(int i=1;i<=c;i++){
            printf("%d",c);
         }
      }
   }
   return 0;
}
