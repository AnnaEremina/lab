#include <math.h>
#include <stdio.h>
double PowerA3(float a)
{
    float b;
    b=pow(a,3);
    return b;
}
int main(){
    printf("Введите 5 чисел для введения в 3 степень: \n");
    double a[5];
    int i;
    for (i = 0; i < 5; i++){
        printf("a[%d] = ", i+1);
        scanf("%lg", &a[i]);
    }
    for(int i=0;i<5;i++)
    {
        a[i]=PowerA3(a[i]);
    }
    for (int i=0;i<5;i++){
        printf("%lg ",a[i]);
    }
    return 0;
}
